{
  "title": "Blockchain Logs",
  "uid": "logs-overview",
  "tags": ["logs", "blockchain"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0},
      "type": "logs",
      "title": "Container Logs - All",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{container_name=~\".+\"}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "direction": "backward",
          "editorMode": "code",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending"
      }
    },
    {
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
      "type": "logs",
      "title": "Signer Nodes Logs",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*signer.*\"}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "direction": "backward",
          "editorMode": "code",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
      "type": "logs",
      "title": "RPC Nodes Logs",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{container_name=~\".*rpc.*\"}",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "direction": "backward",
          "editorMode": "code",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 18},
      "type": "logs",
      "title": "Errors (All Containers)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{container_name=~\".+\"} |~ \"(?i)(error|fatal|panic)\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "direction": "backward",
          "editorMode": "code",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true
      }
    },
    {
      "id": 5,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
      "type": "logs",
      "title": "Warnings (All Containers)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "expr": "{container_name=~\".+\"} |~ \"(?i)(warn|warning)\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "direction": "backward",
          "editorMode": "code",
          "queryType": "range"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "wrapLogMessage": true,
        "enableLogDetails": true
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m"]
  },
  "editable": true
}
